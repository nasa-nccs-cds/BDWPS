# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

GET     /assets/*file            controllers.Assets.versioned(path="/public", file: Asset)

GET     /cwt/status              controllers.WPS.getResultStatus( id: String )
GET     /cwt                     controllers.WPS.execute( version: String ?= "1.0.0", request: String, identifier: String ?= "", storeExecuteResponse: String ?= "true", status: String ?= "false", datainputs: String ?= "" )

GET     /count                   demo.HelloController.count

