# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

GET     /test                       controllers.Application.test
GET     /wps/demo                   controllers.Application.demo
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
GET     /wps/results                controllers.WPS.getResult( id: String, service: String ?= "cds2" )
GET     /wps                        controllers.WPS.execute( version: String ?= "1.0.0", request: String, identifier: String ?= "", service: String ?= "cds2", responseform: String ?= "", storeExecuteResponse: String ?= "true", status: String ?= "false", datainputs: String ?= "" )

GET        /count                   demo.HelloController.count

